  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.1",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.1",
    "dotenv": "^17.2.2",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1"
  }
}
